{
  "name": "vue-starter",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "dependencies": {
    "@flypeng/tool": "^5.0.1",
    "@vueuse/core": "^9.12.0",
    "axios": "^1.3.2",
    "pinia": "^2.0.30",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@flypeng/eslint-config": "^0.0.9",
    "@iconify-json/ep": "^1.1.8",
    "@iconify-json/ic": "^1.1.12",
    "@iconify-json/line-md": "^1.1.22",
    "@iconify-json/material-symbols": "^1.1.28",
    "@iconify-json/mdi": "^1.1.46",
    "@iconify-json/ri": "^1.1.4",
    "@iconify-json/tabler": "^1.1.59",
    "@types/node": "^18.13.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "commitizen": "^4.3.0",
    "eslint": "^8.33.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.1.1",
    "postcss": "^8.4.21",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.2.6",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.14.2",
    "unplugin-icons": "^0.15.2",
    "unplugin-vue-components": "^0.23.0",
    "vite": "^4.1.0",
    "vue-tsc": "^1.0.24"
  },
  "lint-staged": {
    "src/*": "pnpm run lint:fix"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
